{
  "name": "followthrough-ai-integration",
  "version": "1.0.0",
  "description": "Zapier integration for FollowThrough AI - Email triage and meeting analysis with Google OAuth",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "npm run build && zapier test",
    "test:watch": "zapier test --watch",
    "validate": "npm run build && zapier validate",
    "push": "npm run build && zapier push",
    "promote": "zapier promote",
    "logs": "zapier logs",
    "dev": "npm run build && zapier test --debug",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf lib"
  },
  "keywords": [
    "zapier",
    "ai",
    "email",
    "triage",
    "gmail",
    "calendar",
    "automation",
    "followthrough"
  ],
  "author": "FollowThrough AI",
  "license": "MIT",
  "dependencies": {
    "zapier-platform-core": "17.0.0",
    "googleapis": "^128.0.0",
    "axios": "^1.6.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/lodash": "^4.14.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "zapier-platform-cli": "^17.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "zapier": {
    "platformVersion": "17.0.0"
  }
} 